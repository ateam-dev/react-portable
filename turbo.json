{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "bin/**", ".next/**", "!.next/cache/**", ".rp/**"]
    },
    "build:watch": {
      "dependsOn": ["^build"],
      "persistent": true
    },
    "start": {
      "dependsOn": ["build"],
      "persistent": true
    },
    "test": {
      "dependsOn": ["^build"]
    },
    "test:coverage": {
      "dependsOn": ["^build"],
      "outputs": ["coverage/**"]
    },
    "typecheck": {
      "dependsOn": ["^build"]
    }
  }
}