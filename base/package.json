{
  "name": "my-qwik-basic-starter",
  "description": "App with Routing built-in (recommended)",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build -- --react-portal=integrations/react/index.ts",
    "build.preview": "vite build --ssr src/entry.preview.tsx -- --react-portal=integrations/react/index.ts",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr  -- --react-portal=integrations/react/index.ts",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr -- --react-portal=integrations/react/index.ts",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "0.105.0",
    "@builder.io/qwik-city": "~0.104.0",
    "@builder.io/qwik-react": "0.5.0",
    "@types/node": "^18.16.1",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "^3.3.1",
    "typescript": "5.0.4",
    "undici": "5.22.0",
    "vite": "4.3.3",
    "vite-tsconfig-paths": "4.2.0"
  }
}
