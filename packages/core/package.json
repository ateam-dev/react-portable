{
  "name": "@react-portable/core",
  "version": "0.7.0",
  "repository": "git@github.com:ateam-dev/react-portable.git",
  "license": "Apache-2.0",
  "bin": {
    "previewify": "./dist/cli.cjs"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "node": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    },
    "./vite": {
      "require": "./dist/vite.cjs",
      "import": "./dist/vite.mjs",
      "node": "./dist/vite.cjs",
      "default": "./dist/vite.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "vite": [
        "./dist/vite.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "src/templates"
  ],
  "scripts": {
    "build": "retry -n 3 -- unbuild",
    "watch": "nodemon --ext ts,tsx --watch src -x 'npm run build'",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230710.0",
    "@react-portable/client": "0.5.0",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^20.4.1",
    "@types/ws": "^8.5.5",
    "@vitest/coverage-v8": "^0.33.0",
    "memfs": "^4.2.0",
    "msw": "^1.2.2",
    "nodemon": "^3.0.1",
    "retry-cli": "^0.7.0",
    "typescript": "5.2.2",
    "unbuild": "^2.0.0",
    "vitest": "^0.33.0"
  },
  "peerDependencies": {
    "react": ">=18.0.1",
    "react-dom": ">=18.0.0",
    "typescript": ">=4",
    "vite": ">=4"
  },
  "dependencies": {
    "@builder.io/qwik": "1.2.10",
    "@builder.io/qwik-city": "1.2.10",
    "@builder.io/qwik-react": "0.5.0",
    "@hono/qwik-city": "^0.0.4",
    "@react-portable/gateway": "^0.5.0",
    "app-root-path": "^3.1.0",
    "chokidar": "^3.5.3",
    "cloudflared": "^0.4.0",
    "commander": "^11.0.0",
    "get-port": "^7.0.0",
    "hono": "^3.5.4",
    "wrangler": "^3.2.0",
    "ws": "^8.13.0"
  },
  "engines": {
    "node": ">=15.0.0"
  }
}
