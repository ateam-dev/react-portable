    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.3.0",
    "commander": "^10.0.1",
    "vite": "^4.3.5",
    "vite-tsconfig-paths": "4.2.0",
