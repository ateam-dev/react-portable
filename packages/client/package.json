{
  "name": "@react-portable/client",
  "version": "0.0.0",
  "private": true,
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:main": "vite build",
    "build:browser": "vite build -c vite.browser.config.ts",
    "build": "run-p build:*",
    "watch:main": "vite build --watch",
    "watch:browser": "vite build --watch -c vite.browser.config.ts",
    "watch": "run-p watch:*",
    "preview": "vite preview --port 3003",
    "dev": "run-p watch preview",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@vitest/coverage-c8": "^0.31.1",
    "jsdom": "^22.0.0",
    "msw": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "vite": "^4.3.8",
    "vite-plugin-dts": "^2.3.0",
    "vitest": "^0.31.1"
  },
  "dependencies": {
    "writable-dom": "^1.0.3"
  }
}
