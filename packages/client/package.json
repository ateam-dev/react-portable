{
  "name": "@react-portable/client",
  "version": "0.0.2",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:main": "vite build",
    "build:browser": "vite build -c vite.browser.config.ts",
    "build": "run-p build:main build:browser",
    "build:watch:main": "vite build --watch",
    "build:watch:browser": "vite build --watch -c vite.browser.config.ts",
    "build:watch": "run-p build:watch:main build:watch:browser",
    "preview": "vite preview --port 3003",
    "dev": "run-p watch preview",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@vitest/coverage-c8": "^0.31.1",
    "jsdom": "^22.0.0",
    "msw": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^1.7.3",
    "vitest": "^0.31.1"
  },
  "dependencies": {
    "writable-dom": "^1.0.3"
  },
  "peerDependencies": {
    "react": ">=18"
  }
}
